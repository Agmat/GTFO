{
  "manifest_version": 2,
  "name": "GTFO",
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/notification.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": ["<all_urls>", "notifications", "webNavigation", "activeTab"],
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  }
}
